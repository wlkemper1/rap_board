<div class="page-header">
  <h3>Liked: <%= @like.song.title %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @like.user.email %></dd>

      <dt>Song</dt>
      <dd><a href="/songs/<%= @like.song.id %>"><%= @like.song.title %></a></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/likes" class="btn btn-primary">
        Back
      </a>
      <a href="/likes/<%= @like.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_like/<%= @like.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
